{
	"env": {
		"browser": true,
		"es2020": true
	},
	"extends": ["airbnb", "prettier"],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-plusplus": "off",
		"func-names": "off",
		"no-loop-func": "off"
	}
}
